{
  "errors": true, //вывод ошибок активен

  //настройка предупреждений (неуказанные правила по дефолту - true)
  "important": true, //проверять наличие !important при написании стилей
  "box-sizing": false, //не использовать box-sizing: content-box
  "box-model": false, //не выдавать предупреждение о том, что граница делает элемент шире
  "vendor-prefix": true, //проверять наличие вендорных префиксов
  "fallback-colors": false, //не проверять указание цвета для старых браузеров IE8 и ниже
  "ids": true, //проверять, используется ли id при написании стилей
  "known-properties": true, //указывать на свойства, которых нет в спецификации
  "unique-headings": false, //не проверять сколько раз определяются стили для заголовков
  "adjoining-classes": false, //не проверять использование след.селектора .class-name-1.class-name-10
  "order-alphabetical": false, //не проверять, перечислены ли групповые селекторы в алфавитном порядке
  "floats": false, //не проверять наличие плавающих элементов
  "font-sizes": false //не подсчитывать количество объявленных свойств font-size
}